{
  "name": "base-vue",
  "description": "A Vue.js starter project",
  "version": "1.0.0",
  "author": "Peter Cosemans <peter.cosemans@euri.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "start": "nodemon server/server.js",
    "cleanup": "rimraf dist",
    "lint": "eslint 'src/**/*.{js,vue}'; exit 0",
    "format": "prettier --write '**/*.{js,css}'"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.16.0",
    "bootstrap": "^3.3.7",
    "cors": "^2.8.4",
    "express": "^4.14.0",
    "faker": "^4.1.0",
    "morgan": "^1.7.0",
    "sort-on": "^2.0.0",
    "vue": "^2.5.11",
    "vue-router": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^4.1.0",
    "file-loader": "^1.1.4",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
