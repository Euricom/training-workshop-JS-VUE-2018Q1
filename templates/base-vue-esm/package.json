{
  "name": "base-vue",
  "description": "A Vue.js starter project",
  "version": "1.0.0",
  "author": "Peter Cosemans <peter.cosemans@euri.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "cleanup": "rimraf dist",
    "lint": "eslint 'src/**/*.{js,vue}'; exit 0",
    "format": "prettier --write '**/*.{js,css,vue}'"
  },
  "dependencies": {
    "vue": "^2.5.11",
    "vue-router": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^4.1.0",
    "file-loader": "^1.1.4",
    "install": "^0.10.2",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "npm": "^5.6.0",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
